% «crossover»  (to ".crossover")
% (find-es "tex" "crossover")
\def\crossover#1#2#3{%
  \setbox0\hbox{#1}%
  \copy0%
  \hskip -\wd0%
  \setbox1\hbox{\vrule height#2 width\wd0 depth0pt}%
  \raise#3\copy1\relax}
\def\mycrossover#1{\crossover{#1}{0.25pt}{1.75pt}}


